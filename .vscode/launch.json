{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "codelldb",
            "request": "launch",
            "program": "./target/debug/gtd",
            "name": "Debug executable 'gtd'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=gtd",
                    "--package=gtd"
                ],
                "filter": {
                    "name": "gtd",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "codelldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'gtd'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=gtd",
                    "--package=gtd"
                ],
                "filter": {
                    "name": "gtd",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}
